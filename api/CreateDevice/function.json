{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "tableName": "devices",
      "connection": "DeviceStorageConnection",
      "name": "tableBinding",
      "type": "table",
      "direction": "out"
    },
    {
      "name": "blobBinding",
      "type": "blob",
      "path": "images/{filename}",
      "connection": "DeviceStorageConnection",
      "direction": "out"
    }
  ]
}