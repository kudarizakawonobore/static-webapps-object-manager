{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/login",
            "serve": "/.auth/login/aad"
        },
        {
            "route": "/devices",
            "serve": "/devices",
            "allowedRoles": [
                "reader"
            ]
        }
    ],
    "platformErrorOverrides": [
        {
            "errorType": "Unauthenticated",
            "serve": "/login"
        }
    ]
}